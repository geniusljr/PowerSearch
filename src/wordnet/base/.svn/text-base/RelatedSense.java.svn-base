package wordnet.base;

import java.util.*;
public class RelatedSense{
	private String typeName;
	private List<Sense> content;
	public RelatedSense(String name){
		this.typeName = name;
		content = new ArrayList<Sense>();
	}
	public void add(Sense e){
		content.add(e);
	}
	public List<Sense> getRelation(){
		return content;
	}
	public String getType(){
		return typeName;
	}
}